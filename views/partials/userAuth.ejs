<div>
  <h3>Login</h3>
  <form id="login-form" action="/users/admin">
    <label for="username">Username</label>
    <input name="username" placeholder="email" required autofocus id="username">
    <label for="password">Password</label>
    <input name="password" type="password" placeholder="password" required id="password">
    <input name="login" type="submit" value="Login" id="login">
  </form>
</div>
